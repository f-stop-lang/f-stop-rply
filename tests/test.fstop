FN main(frame) -> (
    CONVERT frame 'RGB'
    INVERT frame
)

OPEN URL 'https://cdn.discordapp.com/avatars/522524473447153695/a_04675c590939b234e57bd85c571252e7.gif?size=1024' AS img

ITER (img AS frame) -> (
    CALL main(frame)
)

SAVE img 'test.gif'